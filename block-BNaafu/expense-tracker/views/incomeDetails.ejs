<%- include('partials/header') %>
<main>
  <section>
    <div class="container">
      <h1>Income Details Page</h1>
      <div class="detail-page">
        <h2><span>Income Name : </span> <%= income.incname %></h2>
        <% income.sources.forEach(i => { %>
        <h2><span> Income Source : </span> <%= i %></h2>
        <% }) %>
        <h2><span>Date:</span> <%= date %></h2>
        <h2><span>Amount: </span><%= income.amount %></h2>
        <div class="btn-delete">
          <a href="/income/<%= income.id %>/edit">Edit</a>
          <a href="/income/<%= income.id %>/delete">Delete</a>
        </div>
      </div>
    </div>
  </section>
</main>

<%- include('partials/footer') %>
